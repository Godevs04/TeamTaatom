{
  "expo": {
    "name": "Taatom",
    "slug": "taatom",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "taatom",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "policies/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taatom.app",
      "buildNumber": "3",
      "googleMapsApiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_IOS_KEY}",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you share photos with your friends.",
        "NSCameraUsageDescription": "This app needs access to your camera to let you take photos for posts.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to tag your posts with location information.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to tag your posts with location information.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for video recording.",
        "NSUserTrackingUsageDescription": "We use your data to personalize your experience and show you relevant content. This helps us improve the app and provide better features.",
        "NSPrivacyPolicyURL": "https://taatom.com/privacy",
        "UIBackgroundModes": [
          "audio",
          "location",
          "remote-notification"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "taatom",
              "com.taatom.app"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "mailto",
          "tel",
          "sms"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.taatom.app",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_ANDROID_KEY}"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "taatom",
              "host": "*"
            },
            {
              "scheme": "https",
              "host": "*.taatom.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "Taatom",
      "shortName": "Taatom",
      "lang": "en",
      "scope": "/",
      "themeColor": "#000000",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "description": "Taatom - Social Media App"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos for posts."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "taatom-mobile",
          "organization": "godevs"
        }
      ],
      [
        "expo-updates",
        {
          "username": "taatom"
        }
      ],
      "expo-font"
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "API_BASE_URL": "https://taatombackend-prod-yf2tz.sevalla.app",
      "WEB_SHARE_URL": "https://taatom.com",
      "GOOGLE_MAPS_API_KEY": "",
      "LOGO_IMAGE": "https://res.cloudinary.com/dcvdqhqzc/image/upload/v1766525159/aefbv7kr261jzp4sptel.png",
      "EXPO_PROJECT_ID": "c3b80b3d-23d8-4948-abfa-80963e4192d0",
      "GOOGLE_CLIENT_ID": "139138195988-kclhcnave5urmm7ob2o2erkebp9o4f90.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_IOS": "139138195988-kclhcnave5urmm7ob2o2erkebp9o4f90.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_ANDROID": "139138195988-kclhcnave5urmm7ob2o2erkebp9o4f90.apps.googleusercontent.com",
      "GOOGLE_REDIRECT_URI": "https://auth.expo.io/@taatom/taatom",
      "PRIVACY_POLICY_URL": "https://taatom.com/privacy",
      "TERMS_OF_SERVICE_URL": "https://taatom.com/terms",
      "SUPPORT_URL": "https://taatom.com/contact",
      "router": {},
      "eas": {
        "projectId": "c3b80b3d-23d8-4948-abfa-80963e4192d0"
      }
    },
    "owner": "taatom-technologies"
  }
}
