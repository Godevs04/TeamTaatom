{
  "expo": {
    "name": "Taatom",
    "slug": "taatom",
    "scheme": "taatom",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.taatom.demo",
      "associatedDomains": [
        "applinks:localhost:3000"
      ],
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBV-jFFSI6o--8SiXjzPYon8WH4slor9Co"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos for posts.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select photos for posts.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to tag your posts with location data."
      },
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "package": "com.taatom.demo",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBV-jFFSI6o--8SiXjzPYon8WH4slor9Co"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http",
              "host": "localhost",
              "port": "3000"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos for posts."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "API_BASE_URL": "http://192.168.1.8:3000",
      "GOOGLE_CLIENT_ID": "236289534770-a6be8j239t504vq9o8mt8qgeh2nsu9h0.apps.googleusercontent.com",
      "GOOGLE_CLIENT_SECRET": "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",
      "GOOGLE_REDIRECT_URI": "https://auth.expo.io/@teamgodevs/taatom",
      "GOOGLE_MAPS_API_KEY": "AIzaSyBV-jFFSI6o--8SiXjzPYon8WH4slor9Co",
      "LOGO_IMAGE": "https://res.cloudinary.com/dcvdqhqzc/image/upload/v1756537440/bokj2vfio8cenbo6wfea.png",
      "EXPO_PROJECT_ID": "10a6919b-ba8b-4dfb-b379-d36909e67701",
      "router": {}
    },
    "owner": "teamgodevs"
  }
}
