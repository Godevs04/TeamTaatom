{
  "expo": {
    "name": "Taatom",
    "slug": "taatom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "taatom",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "policies/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taatom.app",
      "buildNumber": "1.0.0",
      "deploymentTarget": "13.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you share photos with your friends.",
        "NSCameraUsageDescription": "This app needs access to your camera to let you take photos for posts.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to tag your posts with location information.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to tag your posts with location information.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for video recording.",
        "NSUserTrackingUsageDescription": "We use your data to personalize your experience and show you relevant content. This helps us improve the app and provide better features.",
        "NSPrivacyPolicyURL": "http://localhost:8081/policies/privacy",
        "UIBackgroundModes": [
          "audio",
          "location",
          "remote-notification"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "taatom",
              "com.taatom.app"
            ]
          }
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.taatom.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBV-jFFSI6o--8SiXjzPYon8WH4slor9Co"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "taatom",
              "host": "*"
            },
            {
              "scheme": "https",
              "host": "*.taatom.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "Taatom",
      "shortName": "Taatom",
      "lang": "en",
      "scope": "/",
      "themeColor": "#000000",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "description": "Taatom - Social Media App"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos for posts."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "taatom-mobile",
          "organization": "godevs"
        }
      ],
      [
        "expo-updates",
        {
          "username": "teamgodevs"
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "API_BASE_URL": "http://192.168.1.9:3000",
      "WEB_SHARE_URL": "http://localhost:8081",
      "GOOGLE_MAPS_API_KEY": "AIzaSyBV-jFFSI6o--8SiXjzPYon8WH4slor9Co",
      "LOGO_IMAGE": "https://res.cloudinary.com/dcvdqhqzc/image/upload/v1756537440/bokj2vfio8cenbo6wfea.png",
      "EXPO_PROJECT_ID": "c3b80b3d-23d8-4948-abfa-80963e4192d0",
      "GOOGLE_CLIENT_ID": "236289534770-vvfj6c8611ci84aja7jsvd67rrj4uprv.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_IOS": "236289534770-vvfj6c8611ci84aja7jsvd67rrj4uprv.apps.googleusercontent.com",
      "GOOGLE_CLIENT_ID_ANDROID": "236289534770-vvfj6c8611ci84aja7jsvd67rrj4uprv.apps.googleusercontent.com",
      "GOOGLE_REDIRECT_URI": "https://auth.expo.io/@teamgodevs/taatom",
      "PRIVACY_POLICY_URL": "http://localhost:8081/policies/privacy",
      "TERMS_OF_SERVICE_URL": "http://localhost:8081/policies/terms",
      "SUPPORT_URL": "http://localhost:8081/support/contact"
    },
    "owner": "taatom-technologies"
  }
}