{
  "name": "taatom-backend",
  "version": "1.0.0",
  "description": "Express.js backend for Taatom mobile app",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:tripVisit": "jest src/__tests__/tripVisitService.test.js",
    "migrate": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "migrate:down": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js down",
    "migrate:status": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js status",
    "migrate:create": "node ./node_modules/migrate-mongo/bin/migrate-mongo.js create",
    "copy-policies": "node scripts/copy-policies.js"
  },
  "keywords": [
    "express",
    "mongodb",
    "cloudinary",
    "jwt",
    "otp"
  ],
  "author": "Taatom",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.947.0",
    "@aws-sdk/s3-request-presigner": "^3.947.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/messaging": "^23.7.0",
    "@sendinblue/client": "^3.3.1",
    "@sentry/node": "^10.29.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^10.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^17.0.1",
    "migrate-mongo": "^12.1.3",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  }
}
